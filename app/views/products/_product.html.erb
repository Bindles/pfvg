<div id="<%= dom_id product %>">
  <p>
    <strong>Handle:</strong>
    <%= product.handle %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Retail price:</strong>
    <%= product.retail_price %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= product.category %>
  </p>

  <p>
    <strong>Path:</strong>
    <%= product.path %>
  </p>

  <p>
    <strong>Default image url:</strong>
    <%= product.default_image_url %>
  </p>

  <p>
    <strong>Currency:</strong>
    <%= product.currency %>
  </p>

  <p>
    <strong>Entity:</strong>
    <%= product.entity_id %>
  </p>

</div>
